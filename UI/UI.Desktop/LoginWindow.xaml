<Window x:Class="UI.Desktop.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UI.Desktop"
        Title="{Binding TitleLogin}"
        mc:Ignorable="d"
        Height="305.859" Width="299.609" DataContext="{Binding LoginVM, Source={StaticResource Locator}}">
    <Grid>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="100,166,0,0" VerticalAlignment="Top" Width="75" Command="{Binding Login}"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="78,111,0,0" TextWrapping="Wrap" Text="{Binding TitleLogin}" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
